{
  "name": "attachmentgenie-timezone",
  "version": "3.0.0",
  "author": "Bram Vogelaar <bram@attachmentgenie.com>",
  "summary": "Puppet timezone Module",
  "license": "MIT",
  "source": "https://github.com/attachmentgenie/attachmentgenie-timezone",
  "project_page": "https://github.com/attachmentgenie/attachmentgenie-timezone",
  "issues_url": "https://github.com/attachmentgenie/attachmentgenie-timezone/issues",
  "description": "Module for configuring timezone.",
  "tags": [
    "timezone",
    "tzdata",
    "localtime"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14",
        "16"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.13.0 < 5.0.0"
    }
  ]
}
